{
    "name": "phpstan\/phpstan-compiler",
    "type": "project",
    "description": "PHAR Compiler for PHPStan",
    "license": [
        "MIT"
    ],
    "require": {
        "php": "^7.1",
        "nette\/neon": "^3.0.0",
        "symfony\/console": "^4.1",
        "symfony\/process": "^4.1",
        "symfony\/filesystem": "^4.1",
        "symfony\/finder": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "PHPStan\\Compiler\\": "src\/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPStan\\Compiler\\": "tests\/"
        }
    },
    "require-dev": {
        "phpunit\/phpunit": "^8.4",
        "phpstan\/phpstan-phpunit": "^0.12.8"
    }
}